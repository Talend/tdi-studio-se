<COMPONENT>
    <HEADER
        PLATEFORM="ALL"
        SERIAL=""
        VERSION="0.102"
        STATUS="ALPHA"
        COMPATIBILITY="ALL"
        AUTHOR="Talend"
        RELEASE_DATE="20050320A"
        STARTABLE="true"
        LOG4J_ENABLED="true">
        <SIGNATURE/>
    </HEADER>

    <FAMILIES>
        <FAMILY>Talend MDM</FAMILY>
    </FAMILIES>

    <DOCUMENTATION>
        <URL />
